  "paths": {
    "/api/user/registration": {
      "post": {
        "summary": "User can register.",
        "consumes": ["application/json"],
        "produces": ["application/json"],
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "type": "string",
                  "example": "kaushikrathod@gmail.com"
                },
                "password": { "type": "string", "example": "Kaushik@123" },
                "first_name": { "type": "string", "example": "Kaushik" },
                "last_name": { "type": "string", "example": "Rathod" },
                "dob": { "type": "string", "example": "2000-07-01" },
                "role": { "type": "string", "example": "User" }
              },
              "required": [
                "email",
                "password",
                "first_name",
                "last_name",
                "dob",
                "role"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "User registered",
            "schema": {
              "type": "object",
              "properties": {
                "status": { "type": "string", "example": "success" },
                "code": { "type": "number", "example": 201 },
                "message": {
                  "type": "string",
                  "example": "User registered successfully."
                }
              }
            }
          }
        }
      }
    },
    "/api/user/login": {
